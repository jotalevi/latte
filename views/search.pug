html
  head
    include head.pug
  body
    include header.pug
    div(class='main-frame')
      div(class='h50')
      div(class='srb')
        each anime in results
          a(href='/anime/' + anime.id)
            div(class='anime-result-div')
              img(src=anime.thumbnail, alt=anime.title, class='minithumbdiv')
              p(class='animetitlepsinres')=anime.title
        div(class='h200')